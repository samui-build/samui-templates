{
  "name": "react-router-mantine",
  "description": "React Router (framework) using Mantine",
  "private": true,
  "type": "module",
  "scripts": {
    "build": "react-router build",
    "ci": "npm run build",
    "dev": "react-router dev",
    "docker:build": "docker build -t react-router-mantine:latest .",
    "docker:push": "docker push react-router-mantine",
    "docker:run": "docker run --rm --name react-router-mantine -p 3000:3000 --env-file .env react-router-mantine:latest",
    "fmt": "prettier --write .",
    "fmt:check": "prettier --check .",
    "lint": "eslint .",
    "lint:fix": "eslint --fix .",
    "start": "react-router-serve ./build/server/index.js",
    "typecheck": "react-router typegen && tsc"
  },
  "dependencies": {
    "@mantine/core": "^7.17.0",
    "@mantine/form": "^7.17.0",
    "@mantine/hooks": "^7.17.0",
    "@mantine/modals": "^7.17.0",
    "@mantine/notifications": "^7.17.0",
    "@react-router/node": "^7.2.0",
    "@react-router/serve": "^7.2.0",
    "isbot": "^5.1.23",
    "lucide-react": "^0.477.0",
    "react": "^19.0.0",
    "react-dom": "^19.0.0",
    "react-router": "^7.2.0"
  },
  "devDependencies": {
    "@eslint/js": "^9.21.0",
    "@react-router/dev": "^7.2.0",
    "@types/node": "^22.13.7",
    "@types/react": "^19.0.10",
    "@types/react-dom": "^19.0.4",
    "eslint": "^9.21.0",
    "globals": "^16.0.0",
    "postcss": "^8.5.3",
    "postcss-preset-mantine": "^1.17.0",
    "postcss-simple-vars": "^7.0.1",
    "prettier": "^3.5.2",
    "react-router-devtools": "^1.1.6",
    "typescript": "^5.7.2",
    "typescript-eslint": "^8.25.0",
    "vite": "^6.2.0",
    "vite-tsconfig-paths": "^5.1.4"
  },
  "license": "MIT",
  "version": "1.0.0"
}
